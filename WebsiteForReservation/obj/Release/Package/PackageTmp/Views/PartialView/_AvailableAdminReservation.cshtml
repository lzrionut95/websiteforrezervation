@model IEnumerable<string>

@{
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            <h4>Dates</h4>
        </th>

    </tr>

    @foreach (string item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(hourSelected => item)
            </td>

            <td>@Html.ActionLink("Reserve", "DoAdminReservation", "Admin", new { hourSelected = item }, FormMethod.Get)</td>

        </tr>
    }

</table>


